{
    "compilerOptions": {
      "moduleResolution": "node",
      "baseUrl": ".",
      "paths": {
        "react": ["node_modules/react"],
        "react/*": ["node_modules/react/*"],
      },
      "rootDir": "./src",
      "target": "ES6",
      "module": "ESNext",
      "jsx": "react",
      "strict": true,
      "esModuleInterop": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true
    },
    "include": ["src/**/*.ts"],
  }
  